/* 툴팁 CSS */
.tooltipOuter {
	position: relative;
	display: inline-block;
}
.tooltipOuter .tooltipDesc {
	visibility: hidden;
	width: 280px;
	background-color: black;
	color: #fff;
	text-align: left;
	border-radius: 6px;
	padding: 5px 0 5px 5px;

	/* Position the tooltip */
	position: absolute;
	z-index: 1;
}
.tooltipOuter:hover .tooltipDesc {
	visibility: visible;
}

.tooltipOuterLeft {
	position: relative;
	display: inline-block;
}
.tooltipOuterLeft .tooltipDesc {
	visibility: hidden;
	width: 280px;
	background-color: black;
	color: #fff;
	text-align: left;
	border-radius: 6px;
	padding: 5px 0 5px 5px;

	/* Position the tooltip */
	position: absolute;
	z-index: 1;
/* 	위치를 왼쪽으로 */
	top: -5px;
	right: 105%; 
}
.tooltipOuterLeft:hover .tooltipDesc {
	visibility: visible;
}
